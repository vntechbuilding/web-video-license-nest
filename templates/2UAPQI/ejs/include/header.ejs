<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title><%= DEFAULT && DEFAULT['meta_title'] && DEFAULT['meta_title']['content'] %></title>
    <meta name="title" property="og:title" content="<%= DEFAULT && DEFAULT['meta_title'] && DEFAULT['meta_title']['content'] %>">
    <meta name="url" property="og:url" content="<%-domainUrl%><%- requestUrl %>" />
    <% if(DEFAULT['meta_image']) { %>
        <meta property="og:image" name="image" content="<%-domainUrl%><%-uploadMetaImageUrl%><%-DEFAULT['meta_image'] %>">
    <% } %>
    <% if(DEFAULT['meta_video']) { %>
    <meta property="og:video" name="video" content="<%-domainUrl%><%-uploadVideoUrl%><%-DEFAULT['meta_video'] %>">
    <% } %>
    <% if (listSchema && listSchema.length) {
        listSchema.forEach(function(schema) { %>
            <script type="application/ld+json">
<%- JSON.stringify(schema, null, 2) %>
            </script>
        <% });
    } %>
    <meta property="og:description" name="description" content="<%= DEFAULT && DEFAULT['meta_description'] && DEFAULT['meta_description']['content'] %>">
    <%- domain && domain.favicons %>
    <%- DEFAULT && DEFAULT['head_javascript'] && DEFAULT['head_javascript']['content'] %>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link rel="stylesheet" href="<%- assetsDir %>css/owl.carousel.min.css" />
    <link rel="stylesheet" href="<%- assetsDir %>css/owl.theme.default.min.css" />
    <link rel="stylesheet" href="<%- assetsDir %>css/main.css" />
</head>
<body>
    <div class="header-container">
        <div class="container">
            <div class="row">
                <div class="col-xs-6 col-sm-5 col-xl-3 col-xxl-2">
                    <a class="logo-image" href="/" title="<%= DEFAULT && DEFAULT['meta_title'] && DEFAULT['meta_title']['content'] %>">
                        <img src="<%- defaultImageTemplateDir %><%- DEFAULT && DEFAULT['logo'] && DEFAULT['logo']['content'] %>" alt="">
                    </a>
                </div>
                <div class="col-xs-6 col-sm-7 col-xl-9 col-xxl-10">
                    <div class="banner-text">
                        <%- DEFAULT && DEFAULT['banner_text'] && DEFAULT['banner_text']['content'] %>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="menu-container">
        <div class="container">
            <%- include('menu') %>
        </div>
    </div>